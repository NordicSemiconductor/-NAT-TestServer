{
  "name": "@nordicsemiconductor/nat-testserver",
  "version": "0.0.0-development",
  "description": "Receives NAT test messages from the NAT-TestFirmware and logs them and timeout occurances to S3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/NAT-TestServer.git"
  },
  "keywords": [
    "NAT-Test",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/NAT-TestServer/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/NAT-TestServer#readme",
  "devDependencies": {
    "@aws-cdk/aws-ec2": "^1.36.0",
    "@aws-cdk/aws-ecr": "^1.36.0",
    "@aws-cdk/aws-ecs": "^1.36.0",
    "@aws-cdk/aws-iam": "^1.36.0",
    "@aws-cdk/aws-s3": "^1.36.0",
    "@aws-cdk/core": "^1.36.0",
    "@bifravst/code-style": "^7.1.8",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/node": "^13.13.4",
    "aws-cdk": "^1.36.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.7",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged && npx tsc"
    }
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.go": [
      "gofmt -w"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
