{
    "type": "object",
    "properties": {
        "op": {
            "type": "string",
            "maxLength": 5
        },
        "ip": {
            "type": "string",
            "oneOf": [
                { "format": "ipv4" },
                { "format": "ipv6" }
            ]
        },
        "cell_id": {
            "type": "integer",
            "minimum": 0
        },
        "ue_mode": {
            "type": "integer",
            "minimum": 0,
            "maximum": 2
        },
        "iccid": {
            "type": "string",
            "pattern": "^89[0-9]{17}([0-9]|F)$"
        },
        "interval": {
            "type": "integer",
            "minimum": 1
        }
    },
    "required": ["op", "ip", "cell_id", "ue_mode", "iccid", "interval"],
    "additionalProperties": false
}
